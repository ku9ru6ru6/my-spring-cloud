server:
  port: 9004

spring:
  application:
    name: zuul-server
  session:
    store-type: redis

eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:9000/eureka/

zuul:
  routes:
    chat-server:
      path: /chat-server/**
      serviceId: chat-server
      sensitiveHeaders:
    feign-server:
      path: /feign-server/**
      serviceId: feign-server
      sensitiveHeaders:

